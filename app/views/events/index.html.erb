<%= link_to "Set up an Event!", new_event_path %> <br/> <hr>

  <% @events.each do |event| %>
    <%= link_to event.event_name, event_path(event) %><br/>
    <%= event.street_address %><br/>
    <%= event.city %><br/>
    <%= event.state %><br/>
    <%= event.zip_code %><br/>
    <%= event.phone_number %><br/>
    <%= link_to "Delete information", event_path(event), data: { confirm: "Are you sure?" }, method: :delete %>
  <br/>

  <% end %>

